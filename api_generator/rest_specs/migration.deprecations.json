    },
    "stability":"stable",
    "visibility":"public",
    "headers":{
      "accept": [ "application/json"]
    },
    "url":{
      "paths":[
        {
          "path":"/_migration/deprecations",
          "methods":[
            "GET"
          ]
        },
        {
          "path":"/{index}/_migration/deprecations",
          "methods":[
            "GET"
          ],
          "parts":{
            "index":{
              "type":"string",
              "description":"Index pattern"
            }
          }
        }
      ]
    },
    "params":{}
  }
}
