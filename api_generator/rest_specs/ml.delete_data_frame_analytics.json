{
  "ml.delete_data_frame_analytics":{
    "documentation":{
      "url":"http://www.elastic.co/guide/en/elasticsearch/reference/current/delete-dfanalytics.html"
    },
    "stability":"experimental",
    "url":{
      "paths":[
        {
          "path":"/_ml/data_frame/analytics/{id}",
          "methods":[
            "DELETE"
          ],
          "parts":{
            "id":{
              "type":"string",
              "description":"The ID of the data frame analytics to delete"
            }
          }
        }
      ]
    },
    "params":{
      "force":{
        "type":"boolean",
        "description":"True if the job should be forcefully deleted",
        "default":false
      }
    }
  }
}
