{
  "ml.delete_datafeed":{
    "documentation":{
      "url":"http://www.elastic.co/guide/en/elasticsearch/reference/current/ml-delete-datafeed.html"
    },
    "stability":"stable",
    "url":{
      "paths":[
        {
          "path":"/_ml/datafeeds/{datafeed_id}",
          "methods":[
            "DELETE"
          ],
          "parts":{
            "datafeed_id":{
              "type":"string",
              "description":"The ID of the datafeed to delete"
            }
          }
        }
      ]
    },
    "params":{
      "force":{
        "type":"boolean",
        "required":false,
        "description":"True if the datafeed should be forcefully deleted"
      }
    }
  }
}
