{
  "watcher.execute_watch":{
    "documentation":{
      "url":"http://www.elastic.co/guide/en/elasticsearch/reference/current/watcher-api-execute-watch.html"
    },
    "stability":"stable",
    "url":{
      "paths":[
        {
          "path":"/_watcher/watch/{id}/_execute",
          "methods":[
            "PUT",
            "POST"
          ],
          "parts":{
            "id":{
              "type":"string",
              "description":"Watch ID"
            }
          }
        },
        {
          "path":"/_watcher/watch/_execute",
          "methods":[
            "PUT",
            "POST"
          ]
        }
      ]
    },
    "params":{
      "debug":{
        "type":"boolean",
        "description":"indicates whether the watch should execute in debug mode",
        "required":false
      }
    },
    "body":{
      "description":"Execution control",
      "required":false
    }
  }
}
