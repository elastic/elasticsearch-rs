{
  "ml.delete_model_snapshot":{
    "documentation":{
      "url":"https://www.elastic.co/guide/en/elasticsearch/reference/current/ml-delete-snapshot.html",
      "description":"Deletes an existing model snapshot."
    },
    "stability":"stable",
    "url":{
      "paths":[
        {
          "path":"/_ml/anomaly_detectors/{job_id}/model_snapshots/{snapshot_id}",
          "methods":[
            "DELETE"
          ],
          "parts":{
            "job_id":{
              "type":"string",
              "description":"The ID of the job to fetch"
            },
            "snapshot_id":{
              "type":"string",
              "description":"The ID of the snapshot to delete"
            }
          }
        }
      ]
    }
  }
}
